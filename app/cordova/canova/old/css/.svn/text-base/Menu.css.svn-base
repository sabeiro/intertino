#drop-menu {
    margin: auto auto;
    padding: 0;
    width: 1100px;
    height:120px;
    top: 0;
    list-style-type: none;
}
ul#menu {
    padding: 0px 10px 0px 10px;
    margin: 0px 0px 100px 0px;
    margin:0 auto 00px auto;
    list-style: none;
    position:relative;
    float:left;
    background-color: #ffff;
    %    font-size:80%
    text-align:center;
    #border:0px solid #ccc;
    #-webkit-box-shadow:rgba(0,0,0,0.2) 0px 0px 5px;
    #-moz-box-shadow:rgba(0,0,0,0.2) 0px 0px 5px;
    #-o-box-shadow:rgba(0,0,0,0.2) 0px 0px 5px;
    #box-shadow:rgba(0,0,0,0.2) 0px 0px 5px
}
ul#menu li {
    margin:0;
    padding:5;
    position: static !Important;
    float:left;
    display: inline;
    border-right: 0px solid #222;
}
ul#menu li a {
    /* float: left; */
    padding:0px 0px 0px 0px;
    color: #333;
    text-transform: uppercase;
    text-decoration:none;
    display: block;
    -webkit-transition: all 0.6s ease;
    -moz-transition: all 0.6s ease;
    -ms-transition: all 0.6s ease;
    -o-transition: all 0.6s ease;
    transition: all 0.6s ease;
}
ul#menu li a:hover {
    background: #ffff;
    color:#7734a7;
}
ul#menu li:hover ul{
    display: block;
}
ul#menu ul {
    float:none;
    font-size:24px;
    display: none;
    position: absolute;
    top: 50px;
    left: 170px;
    margin: -1px 20px 0px 10px;
    background: #ffff;
    padding:0;
    display:block;
    list-style: none;
    opacity: 0;
    visibility: hidden;
    z-index:9999;
    -webkit-transition: all 0.6s ease;
    -moz-transition: all 0.6s ease;
    -ms-transition: all 0.6s ease;
    -o-transition: all 0.6s ease;
    transition: all 0.6s ease;
}
ul#menu li:hover > ul {
    opacity: 1;
    visibility: visible;
}
ul#menu ul li {
    /* float: none; */
    display: block;
    border: 0;
}
ul#menu ul a {
    padding:5px;
    #width: 130px;
    display: block;
    white-space: nowrap;
    float: none;
    text-transform: none;
}
ul#menu ul a:hover {
    background-color: #0186ba;
    color:#fff
}
ul#menu ul ul {
    top: 0;
    left: 150px;
    margin: 0;
}
#gallery {
    position:relative;
    height:0px;
    width:700px;
    align:center;
}
#gallery a {
    float:left;
    position:absolute;
}
#gallery a img {
    border:none;
}
#gallery a.show {
    z-index:600
}
#gallery .caption {
    z-index:600; 
    background-color:#000; 
    color:#ffffff; 
    height:30px; 
    width:100%; 
    position:absolute;
    bottom:0;
    align:center;
}
#gallery .caption .content {
    margin:5px
}
#gallery .caption .content h3 {
    margin:0;
    padding:0;
    color:#1DCCEF;
}
.jimgMenu {
	position: relative;
	width: 700;
	height: 250px;
	overflow: hidden;
	margin: 0px 0px 0px;
}
.jimgMenu ul {
	list-style: none;
	margin: 0px;
	display: block;
	height: 250px;
	width: 1340px;	
}
.jimgMenu ul li {
	float: left;
}
.jimgMenu ul li a {
	text-indent: -1000px;
	background:#FFFFFF none repeat scroll 0%;
	border-right: 2px solid #fff;
	cursor:pointer;
	display:block;
	overflow:hidden;
	width:78px;
	height: 200px;
}
.jimgMenu ul li.landscapes a {
	background: url(../images/Vet5.jpg) repeat scroll 0%;
}
.jimgMenu ul li.people a {
	background: url(../images/Vet6.jpg) repeat scroll 0%;
}
.jimgMenu ul li.nature a {
	background: url(../images/Vet7.jpg) repeat scroll 0%;
}
.jimgMenu ul li.abstract a {
	background: url(../images/Vet8.jpg) repeat scroll 0%;
}
.jimgMenu ul li.urban a {
	background: url(../images/Vet9.jpg) repeat scroll 0%;
	min-width:310px;
	
}
.clear {
	clear: both;
}
